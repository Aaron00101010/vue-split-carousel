{
  "name": "vue-split-carousel",
  "version": "0.0.1",
  "description": "a carousel component of vue, display several carousel-item in the carousel container",
  "main": "dist/vue-split-carousel.common.js",
  "module": "dist/vue-split-carousel.esm.js",
  "directories": {},
  "typings": "types/index.d.ts",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "keywords": [
    "carousel split",
    "carousel",
    "vue",
    "components"
  ],
  "browserslist": [
    "last 2 version",
    "> 0.5%",
    "maintained node versions",
    "not dead",
    "ie > 8"
  ],
  "scripts": {
    "dev": "webpack-dev-server --open --config ./scripts/webpack.config.dev.js",
    "build": "npm run clean && npm run build:umd && npm run build:esm",
    "build:umd": "cross-env NODE_ENV=production webpack --config ./scripts/webpack.config.umd.js --mode production",
    "build:esm": "webpack --config ./scripts/webpack.config.esm.js",
    "clean": "rimraf ./dist",
    "lint": "eslint ./**/*.vue ./**/*.js --fix",
    "test": "npm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aaron00101010/vue-split-carousel.git"
  },
  "author": "Aaron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aaron00101010/vue-split-carousel/issues"
  },
  "homepage": "https://github.com/Aaron00101010/vue-split-carousel#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push":"npm run build"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint ./**/*.vue ./**/*.js --fix",
      "git add"
    ]
  }
}
